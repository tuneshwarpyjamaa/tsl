{
  "version": 2,
  "protectionBypass": {
    "/api/*": true
  },
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/pages/api/run-python.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/run-python",
      "dest": "/api/run-python"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}